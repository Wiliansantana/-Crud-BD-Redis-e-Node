<% include partials/header %>
<% include partials/nav %>
<body>
    <div class="container">
            <% if (typeof message != 'undefined' && message) { %>
                <h2><%= message %></h2>
                <% } %>
    <form method="post" action="/users/add">
    <div class="form-group">
      <label>Id produto</label>
      <input type="text" name="userId" class="form-control" placeholder="22">
    </div>
    <div class="form-group">
      <label>Nome</label>
      <input type="text" name="name" class="form-control" placeholder="Caneta">
    </div>
    <div class="form-group">
      <label>Quantidade</label>
      <input type="text" name="email" class="form-control" aria-describedby="emailHelp" placeholder="4">
      <small id="emailHelp" class="form-text text-muted"></small>
    </div>
    <div class="form-group">
      <label>Cor</label>
      <input type="text" name="password" class="form-control" placeholder="Azul">
    </div>
    <button type="Enviar" class="btn btn-primary">Submit</button>
  </form>
</div>
</body>
</html>
